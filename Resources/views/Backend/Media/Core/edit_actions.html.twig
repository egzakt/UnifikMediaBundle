{% spaceless %}
    <div class="form_actions_bar">
        <ul>
            <li>
                <a href="{% if cancel_route is defined %}{{ cancel_route }}{% else %}{{ path( bundle, { 'section_id': section.id }) }}{% endif %}">{% trans %}Cancel{% endtrans %}</a>
                <span>|</span>
            </li>
            <li>
                <a id="media-delete" href="{% if delete_route is defined %}{{ delete_route }}{% else %}{{ path( entity.getRouteBackend('delete'), entity.getRouteBackendParams() ) }}{% endif %}" rel="{% if delete_route_rel is defined %}{{ delete_route_rel }}{% else %}{{ path( entity.getRouteBackend('delete'), entity.getRouteBackendParams({'message': true}) ) }}{% endif %}" title="{% trans %}Delete{% endtrans %}">
                    {% trans %}Delete{% endtrans %}
                </a>
                <span>|</span>
            </li>
            <li>
                <a href="{% if duplicate_route is defined %}{{ duplicate_route }}{% else %}{{ path( entity.getRouteBackend('duplicate'), entity.getRouteBackendParams())  }}{% endif %}">{% trans %}Duplicate{% endtrans %}</a>
            </li>
        </ul>
        <input class="save" type="submit" value="{% trans %}Save{% endtrans %}" name="save" />
        <input class="save_continue" type="submit" value="{% trans %}Save and continue{% endtrans %}" name="save_continue" />
    </div>
{% endspaceless %}
